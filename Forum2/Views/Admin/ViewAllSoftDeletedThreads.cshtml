@model List<ForumThread>

@{
    ViewData["Title"] = "Deleted Threads";
}

<h1>Admin Panel</h1>


<div>
    <h2>Soft deleted threads</h2>
    <hr />
    <div class="row">
        <div class="col-md-3 mb-2 mb-md-0">
            <partial name="_AdminNavListPartial" />
        </div>
        <div class="col-md-9">
            @foreach(var thread in Model)
            {
                <partial  name="_ThreadListItem" model="thread"/>
            }
        </div>
    </div>
</div>


